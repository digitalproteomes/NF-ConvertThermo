manifest {
    description = 'Convert Thermo RAW MS files to mzXML'
    author = 'Patrick Pedrioli'
}

params {
    raw_folder = 'Data/'
    conv_params = ''
}

process.container = 'digitalproteomes/readw:latest'
docker {
    enabled = true
    sudo = true
    runOptions = '-e LOCAL_USER_ID=$(id -u)'
}
